#include <stdio.h>
int main()
{
    int l,b,area,peri;
    scanf("%d",&l);
    scanf("%d",&b);
    
    area=l*b;
    peri=2*(l+b);
    if(area>peri){
        printf("Area\n");
        printf("%d\n",area);
    }
        else if(peri>area){
            printf("Peri\n");
            printf("%d\n",peri);
        }
            else{
                printf("Eq\n");
                printf("%d\n",area);
            }
	return 0;
}
